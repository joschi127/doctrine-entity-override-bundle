{
  "name": "joschi127/doctrine-entity-override-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle which allows to override entities by using inheritance",
  "keywords": [ "doctrine", "doctrine2", "entity", "override", "inheritance","customize", "mapped-superclass" ],
  "homepage": "https://github.com/joschi127/doctrine-entity-override-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "joschi127",
      "email": "127.joschi@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "symfony/framework-bundle": "^2.7.12|^3.0.6|^4.0",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "^1.6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "symfony/finder": "^2.4|^3.0|^4.0",
    "symfony/monolog-bundle": "^2.4|^3.0|^4.0",
    "monolog/monolog": "^1.0",
    "symfony/swiftmailer-bundle": "^2.3|^3.0",
    "friendsofsymfony/user-bundle": "^2.1"
  },
  "autoload": {
    "psr-4": { "Joschi127\\DoctrineEntityOverrideBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
